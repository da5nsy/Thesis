  file = {C:\\Users\\cege-user\\Zotero\\storage\\56IRBTUX\\Adhikari et al. - 2016 - Rhodopsin and Melanopsin Contributions to the Earl.pdf;C:\\Users\\cege-user\\Zotero\\storage\\6E8TF62U\\article.html}
