  journal = {Color Research \& Application},
  doi = {10.1002/col.20046},
  url = {http://onlinelibrary.wiley.com/doi/10.1002/col.20046/abstract},
  author = {Day, Ellen A. and Taplin, Lawrence and Berns, Roy S.},
  month = oct,
  year = {2004},
  keywords = {color management,display colorimetry,display profiles,LCD colorimetry},
  pages = {365-373},
