  author = {LUCASSEN, MARCEL P. and WALRAVEN, JAN},
  month = sep,
  year = {1996},
  keywords = {Read Soon,color vision,Color constancy,color rendering,Computational vision,Cone-specific contrast,Surface reflectance},
  pages = {2699-2711},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\S8HFKDSM\\LUCASSEN and WALRAVEN - 1996 - Color Constancy under Natural and Artificial Illum.pdf;C:\\Users\\cege-user\\Zotero\\storage\\VEM35SSV\\0042698995003460.html},
