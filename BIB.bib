  month = mar,
  year = {2011},
  keywords = {Light,Photoreceptors,Pupil,Chronobiology,Circadian rhythms,Melatonin,Reflexes,Sleep},
  pages = {e17860},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\DKPRHVD4\\Zele et al. - 2011 - The Circadian Response of Intrinsically Photosensi.pdf;C:\\Users\\cege-user\\Zotero\\storage\\UNLTD5AY\\article.html}
