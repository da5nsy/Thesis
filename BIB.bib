  keywords = {Read Soon},
  pages = {119-137},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\3NIV432C\\Houser et al. - 2009 - A Test of the SP Ratio as a Correlate for Brightn.pdf;C:\\Users\\cege-user\\Zotero\\storage\\3SMSD3FV\\LEUKOS.2009.06.html}
}

