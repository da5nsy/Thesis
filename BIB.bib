@article{mouland_responses_2017,
  title = {Responses to {{Spatial Contrast}} in the {{Mouse Suprachiasmatic Nuclei}}},
  volume = {27},
  issn = {09609822},
  abstract = {A direct retinal projection targets the suprachiasmatic nucleus (SCN) (an important hypothalamic control center). The accepted function of this projection is to convey information about ambient light (irradiance) to synchronize the SCN's endogenous circadian clock with local time and drive the diurnal variations in physiology and behavior [1\textendash{}4]. Here, we report that it also renders the SCN responsive to visual images. We map spatial receptive fields (RFs) for SCN neurons and find that only a minority are excited (or inhibited) by light from across the scene as expected for irradiance detectors. The most commonly encountered units have RFs with small excitatory centers, combined with very extensive inhibitory surrounds that reduce their sensitivity to global changes in light in favor of responses to spatial patterns. Other units have larger excitatory RF centers, but these always cover a coherent region of visual space, implying visuotopic order at the single-unit level. Approximately 75\% of light-responsive SCN units modulate their firing according to simple spatial patterns (drifting or inverting gratings) without changes in irradiance. The time-averaged firing rate of the SCN is modestly increased under these conditions, but including spatial contrast did not significantly alter the circadian phase resetting efficiency of light. Our data indicate that the SCN contains information about irradiance and spatial patterns. This newly appreciated sensory capacity provides a mechanism by which behavioral and physiological systems downstream of the SCN could respond to visual images [5].},
  language = {en},
  number = {11},
  urldate = {2019-04-05},
  journal = {Current Biology},
  doi = {10.1016/j.cub.2017.04.039},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0960982217304839},
  author = {Mouland, Joshua W. and Stinchcombe, Adam R. and Forger, Daniel B. and Brown, Timothy M. and Lucas, Robert J.},
  month = jun,
  year = {2017},
  pages = {1633-1640.e3},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\8ZI5549C\\Mouland et al. - 2017 - Responses to Spatial Contrast in the Mouse Suprach.pdf}
}

