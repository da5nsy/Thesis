  author = {McManus, David D. and Lee, Jinseok and Maitas, Oscar and Esa, Nada and Pidikiti, Rahul and Carlucci, Alex and Harrington, Josephine and Mick, Eric and Chon, Ki H.},
  month = mar,
  year = {2013},
  keywords = {Atrial fibrillation,Detection,Smartphone,Technology},
  pages = {315-319},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\DH2FFX47\\McManus et al. - 2013 - A novel application for the detection of an irregu.pdf;C:\\Users\\cege-user\\Zotero\\storage\\T3PM9JSN\\S154752711201435X.html}
