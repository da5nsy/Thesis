@article{maciver_massive_2017,
  title = {Massive Increase in Visual Range Preceded the Origin of Terrestrial Vertebrates},
  issn = {0027-8424, 1091-6490},
  abstract = {The evolution of terrestrial vertebrates, starting around 385 million years ago, is an iconic moment in evolution that brings to mind images of fish transforming into four-legged animals. Here, we show that this radical change in body shape was preceded by an equally dramatic change in sensory abilities akin to transitioning from seeing over short distances in a dense fog to seeing over long distances on a clear day. Measurements of eye sockets and simulations of their evolution show that eyes nearly tripled in size just before vertebrates began living on land. Computational simulations of these animal's visual ecology show that for viewing objects through water, the increase in eye size provided a negligible increase in performance. However, when viewing objects through air, the increase in eye size provided a large increase in performance. The jump in eye size was, therefore, unlikely to have arisen for seeing through water and instead points to an unexpected hybrid of seeing through air while still primarily inhabiting water. Our results and several anatomical innovations arising at the same time suggest lifestyle similarity to crocodiles. The consequent combination of the increase in eye size and vision through air would have conferred a 1 million-fold increase in the amount of space within which objects could be seen. The ``buena vista'' hypothesis that our data suggest is that seeing opportunities from afar played a role in the subsequent evolution of fully terrestrial limbs as well as the emergence of elaborated action sequences through planning circuits in the nervous system.},
  language = {en},
  urldate = {2019-04-11},
  journal = {Proceedings of the National Academy of Sciences},
  doi = {10.1073/pnas.1615563114},
  url = {https://www.pnas.org/content/early/2017/03/06/1615563114},
  author = {MacIver, Malcolm A. and Schmitz, Lars and Mugan, Ugurcan and Murphey, Todd D. and Mobley, Curtis D.},
  month = mar,
  year = {2017},
  keywords = {vision,fishtetrapod transition,prospective cognition,terrestriality,visual ecology},
  pages = {201615563},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\ZCBNCWMK\\MacIver et al. - 2017 - Massive increase in visual range preceded the orig.pdf;C:\\Users\\cege-user\\Zotero\\storage\\JHWSQG8W\\1615563114.html},
