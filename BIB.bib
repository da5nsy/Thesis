@article{allen_melanopsin_2017,
  title = {Melanopsin {{Contributions}} to the {{Representation}} of {{Images}} in the {{Early Visual System}}},
  issn = {0960-9822},
  abstract = {Summary
Melanopsin photoreception enhances retinal responses to variations in ambient light (irradiance) and drives non-image-forming visual reflexes such as circadian entrainment [1\textendash{}6]. Melanopsin signals also reach brain regions responsible for form vision [7\textendash{}9], but melanopsin's contribution, if any, to encoding visual images remains unclear. We addressed this deficit using principles of receptor silent substitution to present images in which visibility for melanopsin versus rods+cones was independently modulated, and we recorded evoked responses in the mouse dorsal lateral geniculate nucleus (dLGN; thalamic relay for cortical vision). Approximately 20\% of dLGN units responded to patterns visible only to melanopsin, revealing that melanopsin signals alone can convey spatial information. Spatial receptive fields (RFs) mapped using~melanopsin-isolating stimuli had ON centers with diameters {$\sim$}13\textdegree. Melanopsin and rod+cone responses differed in the temporal domain, and responses to slow changes in radiance (\&lt;0.9~Hz) and stationary images were deficient when stimuli were rendered invisible for melanopsin. We employed these data to devise and test a mathematical model of melanopsin's involvement in form vision and applied it, along with further experimental recordings, to explore melanopsin signals under simulated active view of natural scenes. Our findings reveal that melanopsin enhances the thalamic representation of scenes containing local correlations in radiance, compensating for the high temporal frequency bias of cone vision and the negative correlation between magnitude and frequency for changes in direction of view. Together, these data reveal a distinct melanopsin contribution to encoding visual images, predicting that, under natural view, melanopsin augments the early visual system's ability to encode patterns over moderate spatial scales.},
  urldate = {2017-05-19},
  journal = {Current Biology},
  doi = {10.1016/j.cub.2017.04.046},
  url = {http://www.sciencedirect.com/science/article/pii/S0960982217304918},
  author = {Allen, Annette E. and Storchi, Riccardo and Martial, Franck P. and Bedford, Robert A. and Lucas, Robert J.},
  year = {2017},
  keywords = {Read Soon,melanopsin,dorsal lateral geniculate nucleus,intrinsically photosensitive retinal ganglion cells,metameric light stimuli,mouse vision,receptive fields,receptor silent substitution,spatial vision},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\63NHFNW5\\Allen et al. - Melanopsin Contributions to the Representation of .pdf;C:\\Users\\cege-user\\Zotero\\storage\\NCFX94BZ\\Allen et al. - Melanopsin Contributions to the Representation of .html},
