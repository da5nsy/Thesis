  keywords = {Read Soon,Museum lighting,Absorbance,Color change,Degradation,LED,Light damage,Paint,Spectrum},
  pages = {294-303},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\ZBEQWQFC\\Lunz et al. - 2017 - Can LEDs help with art conservation  Impact of d.pdf;C:\\Users\\cege-user\\Zotero\\storage\\SPAKJ5ZZ\\00393630.2016.html}
