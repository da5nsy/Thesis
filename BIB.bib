  keywords = {Computer Science - Learning,Quantitative Biology - Neurons and Cognition,Statistics - Machine Learning},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\P7PTUPXB\\Klindt et al. - 2017 - Neural system identification for large populations.pdf;C:\\Users\\cege-user\\Zotero\\storage\\WBCUC8MK\\1711.html}
