  url = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0066480},
  author = {Weng, Shijun and Estevez, Maureen E. and Berson, David M.},
  month = jun,
  year = {2013},
  keywords = {Read Soon,retina,Retinal Ganglion Cells,Action potentials,Ganglion cells,Gap junctions,Mouse models,Primates,Synapses},
  pages = {e66480},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\I2QPF4XK\\Weng et al. - 2013 - Mouse Ganglion-Cell Photoreceptors Are Driven by t.pdf;C:\\Users\\cege-user\\Zotero\\storage\\H9UDKF45\\article.html}
}

