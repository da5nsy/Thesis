  url = {https://pubs.acs.org/doi/10.1021/bi3004999},
  author = {Matsuyama, Take and Yamashita, Takahiro and Imamoto, Yasushi and Shichida, Yoshinori},
  month = jul,
  year = {2012},
  pages = {5454-5462},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\VYKEUS48\\Matsuyama et al. - 2012 - Photochemical Properties of Mammalian Melanopsin.pdf}
}

@article{stabio_m5_2018,
  title = {The {{M5 Cell}}: {{A Color}}-{{Opponent Intrinsically Photosensitive Retinal Ganglion Cell}}},
