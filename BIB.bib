  author = {Brainard, George C. and Hanifin, John P. and Rollag, Mark D. and Greeson, Jeffrey and Byrne, Brenda and Glickman, Gena and Gerner, Edward and Sanford, Britt},
  month = jan,
  year = {2001},
  pages = {433-436},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\KZ3PCFIS\\Brainard et al. - 2001 - Human Melatonin Regulation Is Not Mediated by the .pdf;C:\\Users\\cege-user\\Zotero\\storage\\BVUSJU6G\\jcem.86.1.html}
