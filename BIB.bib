@article{smolders_higher_2012,
  series = {Tufts {{University}} Special Section},
  title = {A Higher Illuminance Induces Alertness Even during Office Hours: {{Findings}} on Subjective Measures, Task Performance and Heart Rate Measures},
  volume = {107},
  issn = {0031-9384},
  shorttitle = {A Higher Illuminance Induces Alertness Even during Office Hours},
  abstract = {Nocturnal white light exposure has shown marked results on subjective and objective indicators of alertness, vitality and mood, yet effects of white light during daytime and under usual office work conditions have not been investigated extensively. The current study employed a mixed-group design (N = 32), testing effects of two illuminance levels (200 lx or 1000 lx at eye level, 4000 K) during one hour of morning versus afternoon exposure. In four repeated blocks, subjective reports, objective performance and physiological arousal were measured. Results showed effects of illuminance on subjective alertness and vitality, sustained attention in tasks, and heart rate and heart rate variability. Participants felt less sleepy and more energetic in the high versus the low lighting condition, had shorter reaction times on the psychomotor vigilance task and increased physiological arousal. Effects of illuminance on the subjective measures, as well as those on heart rate were not dependent on time of day or duration of exposure. Performance effects were most pronounced in the morning sessions and towards the end of the one-hour exposure period. The effect on heart rate variability was also most pronounced at the end of the one-hour exposure. The results demonstrate that even under normal, i.e., neither sleep nor light deprived conditions, more intense light can improve feelings of alertness and vitality, as well as objective performance and physiological arousal.},
  number = {1},
  urldate = {2016-05-12},
  journal = {Physiology \& Behavior},
  doi = {10.1016/j.physbeh.2012.04.028},
  url = {http://www.sciencedirect.com/science/article/pii/S0031938412001874},
  author = {Smolders, K. C. H. J. and {de Kort}, Y. A. W. and Cluitmans, P. J. M.},
  month = aug,
  year = {2012},
  keywords = {Lighting,Alertness,Daytime,Duration of exposure,Heart Rate,Performance},
  pages = {7-16},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\9CCBZPJE\\Smolders et al. - 2012 - A higher illuminance induces alertness even during.pdf;C:\\Users\\cege-user\\Zotero\\storage\\GUKWTETV\\S0031938412001874.html}
