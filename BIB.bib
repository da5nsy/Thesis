@article{carvalho_genetic_2017,
  title = {The {{Genetic}} and {{Evolutionary Drives}} behind {{Primate Color Vision}}},
  volume = {5},
  issn = {2296-701X},
  abstract = {Primate color vision is based on two to three cone types in the retina, each expressing a different class of visual pigment, making them the only mammals that possess trichromacy. These pigment classes are the short wavelength-sensitive (SWS1) pigment and the long wavelength-sensitive (LWS) pigment, orthologues of the same pigments found in many other vertebrates, as well as the middle wavelength-sensitive (MWS) pigment, a paralogue to the LWS pigment. Trichromacy was achieved differently in Old World and New World primates. In Old World primates, a duplication of the LWS opsin gene occurred giving rise to a ``red-sensitive'' or L pigment and a ``green-sensitive'' or M pigment. Their corresponding L and M genes are adjacent on the X chromosome which, together with their high sequence homology, is the underlying cause for the high frequency of red-green color blindness seen in humans. In New World primates and prosimians, however, the mechanism leading to trichromacy, with one exception, is based on a single polymorphic LWS gene, from which different allelic variants encode pigments with differing spectral peaks. X chromosome inactivation limits expression to just one gene per photoreceptor meaning that trichromacy is only seen in females; while all male are red-green color blind. Despite several leading hypothesis, the reasons for the different evolutionary paths taken by Old and New World primates for trichromacy are still unclear and remain to be confirmed.},
  language = {English},
  urldate = {2018-05-08},
  journal = {Frontiers in Ecology and Evolution},
  doi = {10.3389/fevo.2017.00034},
  url = {https://www.frontiersin.org/articles/10.3389/fevo.2017.00034/full},
  author = {Carvalho, Livia S. and Pessoa, Daniel M. A. and Mountford, Jessica K. and Davies, Wayne I. L. and Hunt, David M.},
  year = {2017},
  keywords = {Dichromacy,Trichromacy,evolution,Ecology,Visual opsins,visual pigments},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\KB9JI99I\\Carvalho et al. - 2017 - The Genetic and Evolutionary Drives behind Primate.pdf}
}

