  url = {http://www.sciencedirect.com/science/article/pii/S0042698984800036},
  author = {Lythgoe, J. N.},
  month = jan,
  year = {1984},
  keywords = {Read Soon,Colour vision,Extra-retinal receptors,noise Rods Cones,Visual pigments Visual},
  pages = {1539-1550},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\SKBZV4JG\\Lythgoe - 1984 - Visual pigments and environmental light.pdf;C:\\Users\\cege-user\\Zotero\\storage\\C4XYKRRB\\S0042698984800036.html}
