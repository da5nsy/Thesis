  keywords = {Read Soon,Color vision,Image resolution,Ideal observers,Spatial resolution,Stray light,Visible light},
  pages = {B14-B24},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\MVNVI77E\\Foster et al. - 2009 - Approaching ideal observer efficiency in using col.pdf;C:\\Users\\cege-user\\Zotero\\storage\\UATES9LT\\abstract.html}
