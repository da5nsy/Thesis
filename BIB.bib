  year = {17-Apr-2015},
  keywords = {Light,Neurons,Photons,Color vision,Circadian rhythms,Body temperature,Statistical data,Twilight},
  pages = {e1002127},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\6V2P6YJJ\\Walmsley et al. - 2015 - Colour As a Signal for Entraining the Mammalian Ci.pdf;C:\\Users\\cege-user\\Zotero\\storage\\PXT7MIED\\article.html}
