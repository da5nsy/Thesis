  url = {http://dx.doi.org/10.1021/ac102424h},
  author = {Monico, Letizia and {Van der Snickt}, Geert and Janssens, Koen and De Nolf, Wout and Miliani, Costanza and Verbeeck, Johan and Tian, He and Tan, Haiyan and Dik, Joris and Radepont, Marie and Cotte, Marine},
  month = feb,
  year = {2011},
  pages = {1214-1223},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\V96WEGGW\\Monico et al. - 2011 - Degradation Process of Lead Chromate in Paintings .pdf;C:\\Users\\cege-user\\Zotero\\storage\\KM3IP73U\\ac102424h.html},
