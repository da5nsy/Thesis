}

@phdthesis{shimozaki_adaptation_1994,
  address = {{United States -- California}},
  type = {Ph.{{D}}.},
  title = {Adaptation and Simultaneous Contrast Effects on Lightness Judgments},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {In a series of studies on lightness perception, subjects performed a discrimination between two simulated reflectances appearing under a simulated illumination modulating sinusoidally. By manipulating the period of modulation, the time dependence of the lightness judgment was measured. If the modulations of illumination are faster than the operation of a mechanism responsible for lightness perception, the mechanism cannot compensate for the modulation, and lightness discrimination performance should suffer. Thus, the time course of lightness perception can be assessed by measuring increases of performance as the period of modulation increases.
There are three classes of mechanisms hypothesized to account for lightness perception which each have specific time courses; multiplicative light adaptation (fast, 50 to 200 ms), subtractive light adaptation (fast and slow, up to 30 sec), and simultaneous contrast (instantaneous).
Four main experiments were performed. To separate the effect of simultaneous contrast from light adaptation, targets were placed either adjacent to the background (Direct Contrast), or with a 1 degree dark annulus between the target and the background (Annulus); the annulus removes simultaneous contrast cues. Also, the targets were either increments or decrements relative to the background. Thus, the four experiments are Direct Contrast Increment, Direct Contrast Decrement, Annulus Increment, and Annulus Decrement.
Results show that simultaneous contrast is the predominant cue for lightness perception in these experiments. A control study comparing Annulus and Direct Contrast conditions for fixed illuminations also suggest strongly the importance of simultaneous contrast. Without simultaneous contrast cues (Annulus), small effects upon lightness perception occur for periods up to 120 sec. These long-term effects most likely correspond to slow adaptation mechanisms (subtractive). No difference was found between increment and decrement targets.},
  language = {English},
  urldate = {2019-11-08},
  school = {University of California, Los Angeles},
  url = {https://search.proquest.com/pqdtglobal/docview/304093765/abstract/4094217979884339PQ/13},
  author = {Shimozaki, Steven Sesshu},
  year = {1994},
  keywords = {Psychology},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\E8ZJ5B6Q\\Shimozaki - 1994 - Adaptation and simultaneous contrast effects on li.pdf}
}

@article{bauml_ratio_1993,
  title = {A Ratio Principle for a Red/Green and a Yellow/Blue Channel?},
  volume = {53},
  issn = {1532-5962},
  abstract = {There is strong empirical evidence that, under adaptation to another achromatic color stimulus, the lightness of an achromatic color stimulus depends on the ratio of the luminances of the two stimuli. In the present study, the suitability of this ratio principle is tested for two chromatic postreceptoral opponent channels. A Hering red/green channel and a non-Hering yellow/blue channel are specified as chromatic channels. The yellow/blue channel is defined by extrapolating the plane corresponding to unique green-white linearly to the reddish part of color space, using the plane's surface as the channel's equilibria. The experiment was run on an isoluminant plane, measured individually for each observer. Moving along an observer's measured opponent axes, eight adaptation stimuli were selected for each channel and spanned the whole range of the channel's coordinates. Red/green equilibria or yellow/blue equilibria were measured as excursions along the adaptation axes. For both presumed channels, the ratios of the equilibrium coordinates of test and adaptation stimuli were essentially constant. This supports the principle's suitability. However, small asymmetries were found with respect to each channel's opponent hues. The status of the proposed yellow/blue channel is discussed, as are conditions that might have favored the present findings.},
  language = {en},
  number = {3},
  urldate = {2019-11-08},
  journal = {Perception \& Psychophysics},
  doi = {10.3758/BF03205188},
  url = {https://doi.org/10.3758/BF03205188},
  author = {B{\"a}uml, Karl-Heinz},
  month = may,
  year = {1993},
  keywords = {Adaptation Stimulus,Chromatic Adaptation,Color Space,Color Stimulus,Test Stimulus},
  pages = {338-344},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\SYE8R3U2\\Buml - 1993 - A ratio principle for a redgreen and a yellowblu.pdf}
}

