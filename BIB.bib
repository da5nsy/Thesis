@article{webster_adaptation_1997,
  title = {Adaptation and the Color Statistics of Natural Images},
  volume = {37},
  issn = {0042-6989},
  abstract = {Color perception depends profoundly on adaptation processes that adjust sensitivity in response to the prevailing pattern of stimulation. We examined how color sensitivity and appearance might be influenced by adaptation to the color distributions characteristic of natural images. Color distributions were measured for natural scenes by sampling an array of locations within each scene with a spectroradiometer, or by recording each scene with a digital camera successively through 31 interference filters. The images were used to reconstruct the L, M and S cone excitation at each spatial location, and the contrasts along three post-receptoral axes [L + M, L - M or S - (L + M)]. Individual scenes varied substantially in their mean chromaticity and luminance, in the principal color-luminance axes of their distributions, and in the range of contrasts in their distributions. Chromatic contrasts were biased along a relatively narrow range of bluish to yellowish-green angles, lying roughly between the S - (L + M) axis (which was more characteristic of scenes with lush vegetation and little sky) and a unique blue-yellow axis (which was more typical of arid scenes). For many scenes L - M and S - (L + M) signals were highly correlated, with weaker correlations between luminance and chromaticity. We use a two-stage model (von Kries scaling followed by decorrelation) to show how the appearance of colors may be altered by light adaptation to the mean if the distributions and by contrast adaptation to the contrast range and principal axes of the distributions; and we show that such adjustments are qualitatively consistent with empirical measurements of asymmetric color matches obtained after adaptation to successive random samples drawn from natural distributions of chromaticities and lightnesses. Such adaptation effects define the natural range of operating states of the visual system.},
  number = {23},
  urldate = {2015-08-03},
  journal = {Vision Research},
  doi = {10.1016/S0042-6989(97)00125-9},
  url = {http://www.sciencedirect.com/science/article/pii/S0042698997001259},
  author = {Webster, Michael A. and Mollon, J. D.},
  month = dec,
  year = {1997},
  keywords = {Read Soon,chromatic adaptation,color appearance,color vision,Contrast adaptation,Light adaptation,Natural images},
  pages = {3283-3298},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\MXCJ4RVJ\\Webster and Mollon - 1997 - Adaptation and the color statistics of natural ima.pdf;C:\\Users\\cege-user\\Zotero\\storage\\TTC6WMDI\\Webster and Mollon - 1997 - Adaptation and the color statistics of natural ima.pdf;C:\\Users\\cege-user\\Zotero\\storage\\CA94DCVP\\S0042698997001259.html;C:\\Users\\cege-user\\Zotero\\storage\\KXC9NKS9\\S0042698997001259.html}
}

@article{rinner_time_2000,
  title = {Time Course of Chromatic Adaptation for Color Appearance and Discrimination},
  volume = {40},
  issn = {0042-6989},
  abstract = {Adaptation to a steady background has a profound effect on both color appearance and discrimination. We determined the temporal characteristics of chromatic adaptation for appearance and discrimination along different color directions. Subjects were adapted to a large uniform background made up of a CRT screen and a 45\texttimes{}64\textdegree{} wall, illuminated by computer controlled lamps. After an instant change in background color along a red\textendash{}green or blue\textendash{}yellow color axis, we measured thresholds for the detection of increments along the same axes at fixed times between 25 ms and 121 s. Analogously, color appearance was determined using achromatic matching. Three components of adaptation could be identified by their temporal characteristics. A slow exponential time course of adaptation with a half-life of about 20 s was common to appearance and discrimination. A faster component with a half-life of 40\textendash{}70 ms \textemdash{} probably due to photoreceptor adaptation \textemdash{} was also common to both. Exclusive for color appearance, there was a third, extremely rapid mechanism with a half-life faster than 10 ms. This instantaneous process explained more than 50\% of total adaptation for color appearance and could be shown to act in a multiplicative manner. We conclude that this instantaneous adaptation mechanism for color appearance is situated at a later processing stage, after mechanisms common to appearance and discrimination, and is based on multiplicative spatial interactions rather than on local, temporal adaptational processes. Color appearance, and thus color constancy, seems to be determined in large part by cortical computations.},
  number = {14},
  urldate = {2015-12-11},
  journal = {Vision Research},
  doi = {10.1016/S0042-6989(00)00050-X},
  url = {http://www.sciencedirect.com/science/article/pii/S004269890000050X},
  author = {Rinner, Oliver and Gegenfurtner, Karl R},
  month = jun,
  year = {2000},
  keywords = {chromatic adaptation,color appearance,Cones,Color constancy,chromatic induction,Color discrimination},
  pages = {1813-1826},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\B6VDCZQZ\\Rinner and Gegenfurtner - 2000 - Time course of chromatic adaptation for color appe.pdf;C:\\Users\\cege-user\\Zotero\\storage\\DEJBTBDM\\Rinner and Gegenfurtner - 2000 - Time course of chromatic adaptation for color appe.pdf;C:\\Users\\cege-user\\Zotero\\storage\\W5JR2MPP\\Rinner - Time course of chromatic adaptation for color appearance - VisRes 2000.pdf;C:\\Users\\cege-user\\Zotero\\storage\\XBNN67KF\\Rinner and Gegenfurtner - 2000 - Time course of chromatic adaptation for color appe.pdf;C:\\Users\\cege-user\\Zotero\\storage\\88IT9PJB\\S004269890000050X.html;C:\\Users\\cege-user\\Zotero\\storage\\QH7IHQE8\\S004269890000050X.html;C:\\Users\\cege-user\\Zotero\\storage\\R4TA8BME\\S004269890000050X.html}
