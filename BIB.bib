  doi = {10.1002/wat2.1388},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/wat2.1388},
  author = {Jechow, Andreas and H{\"o}lker, Franz},
  keywords = {ALAN,artificial light at night,light measurement,light pollution},
  pages = {e1388},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\A3UU2EZ9\\Jechow and Hlker - How dark is a river Artificial light at night in .pdf;C:\\Users\\cege-user\\Zotero\\storage\\VL3GPZN2\\wat2.html}
