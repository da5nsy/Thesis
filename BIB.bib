  file = {C:\\Users\\cege-user\\Zotero\\storage\\PD9WYFQ2\\jphil_2018_0115_0004_0187_0214.pdf;C:\\Users\\cege-user\\Zotero\\storage\\X9RTZRTX\\purchase.html},
  doi = {10.5840/jphil2018115413}
}

