  author = {Worthey, James A.},
  month = feb,
  year = {2004},
  keywords = {Colorimetry,Lighting,color rendering,ASTM,fluorescent lighting,illuminants,linear model,matrix R,neodymium glass,opponent color,prime colors,standards},
  pages = {43-56},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\H2VMKUWP\\Worthey - 2004 - Color rendering A calculation that estimates colo.pdf;C:\\Users\\cege-user\\Zotero\\storage\\R3J2G54A\\abstract.html}
