  pages = {1209-1221},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\CZVCQCM6\\Finlayson et al. - 2001 - Color by correlation a simple, unifying framework.pdf;C:\\Users\\cege-user\\Zotero\\storage\\YNMZJ88H\\969113.html}
