  pages = {712-148},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\GD8SUNR7\\Shepard - 2001 - Shepard's Response On the possibility of universal.pdf;C:\\Users\\cege-user\\Zotero\\storage\\P9URRJME\\Shepard - 2001 - Shepard's Response On the possibility of universal.pdf;C:\\Users\\cege-user\\Zotero\\storage\\6TG3ELUQ\\61A222E94172F64C3ADD07CDEA8D23E4.html;C:\\Users\\cege-user\\Zotero\\storage\\8GMVK3ZS\\61A222E94172F64C3ADD07CDEA8D23E4.html}
