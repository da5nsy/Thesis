  url = {http://www.sciencedirect.com/science/article/pii/S030626191500553X},
  author = {M{\'e}ndez Echenagucia, Tom{\'a}s and Capozzoli, Alfonso and Cascone, Ylenia and Sassone, Mario},
  month = sep,
  year = {2015},
  keywords = {Building energy performance,Building energy optimization,Building envelope,Early design stage,Genetic algorithm,Multi-objective optimization},
  pages = {577-591},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\W4IEB37G\\Mndez Echenagucia et al. - 2015 - The early design stage of a building envelope Mul.pdf;C:\\Users\\cege-user\\Zotero\\storage\\SLE9IAYU\\S030626191500553X.html}
