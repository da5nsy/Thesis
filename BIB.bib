  author = {Llenas, Aleix and Carreras, Josep},
  month = mar,
  year = {2019},
  pages = {035105},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\P4MTJJBH\\Llenas and Carreras - 2019 - Arbitrary spectral matching using multi-LED lighti.pdf;C:\\Users\\cege-user\\Zotero\\storage\\JEHX5Y7W\\1.OE.58.3.035105.html}
