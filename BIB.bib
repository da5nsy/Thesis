@article{hurlbert_biological_2007,
  title = {Biological Components of Sex Differences in Color Preference},
  volume = {17},
  issn = {0960-9822},
  abstract = {Summary
The long history of color preference studies has been described as ``bewildering, confused and contradictory'' [1]. Although recent studies [1\textendash{}3] tend to agree on a universal preference for `blue', the variety and lack of control in measurement methods have made it difficult to extract a systematic, quantitative description of preference. Furthermore, despite abundant evidence for sex differences in other visual domains, and specifically in other tasks of color perception [4,5], there is no conclusive evidence for the existence of sex differences in color preference. This fact is perhaps surprising, given the prevalence and longevity of the notion that little girls differ from boys in preferring `pink' [6]. Here we report a robust, cross-cultural sex difference in color preference, revealed by a rapid paired-comparison task. Individual color preference patterns are summarized by weights on the two fundamental neural dimensions that underlie color coding in the human visual system. We find a consistent sex difference in these weights, which, we suggest, may be linked to the evolution of sex-specific behavioral uses of trichromacy.},
  number = {16},
  urldate = {2015-07-25},
  journal = {Current Biology},
  doi = {10.1016/j.cub.2007.06.022},
  url = {http://www.sciencedirect.com/science/article/pii/S096098220701559X},
  author = {Hurlbert, Anya C. and Ling, Yazhu},
  month = aug,
  year = {2007},
  pages = {R623-R625},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\25GHF66A\\Hurlbert and Ling - 2007 - Biological components of sex differences in color .pdf;C:\\Users\\cege-user\\Zotero\\storage\\7VCATB55\\S096098220701559X.html}
}

@incollection{hurlbert_color_2004,
  series = {The Roots of Visual Awareness: A Festschrift in Honour of {{Alan Cowey}}},
  title = {Color Contrast: A Contributory Mechanism to Color Constancy},
  volume = {144},
  shorttitle = {Color Contrast},
  abstract = {Color constancy \textemdash{} by which objects tend to appear the same color under changes in illumination \textemdash{} is most likely achieved by several mechanisms, operating at different levels in the visual system. One powerful contributory mechanism is simultaneous spatial color contrast. Under changes in natural illumination the spatial ratios of within-type cone excitations between natural surfaces tend to be preserved (Foster and Nascimento, 1994); therefore, the neural encoding of colors as spatial contrasts tends to achieve constancy. Several factors are known to influence the strength of chromatic contrast induction between surfaces, including their relative luminance, spatial scale, spatial configuration and context (Ware and Cowan, 1982; Zaidi et al., 1991). Here we test the hypothesis that color contrast is weakened by differences between surfaces which indicate that they may be under distinct illuminants. We summarize psychophysical measurements of the effects of relative motion, relative depth and texture differences on chromatic contrast induction. Of these factors, only texture differences between surfaces weaken chromatic contrast induction. We also consider neurophysiological and neuropsychological evidence and conclude that the mechanisms which mediate local chromatic contrast effects are sited at low levels in the visual system, in primary visual cortex (V1) or below, prior to image segmentation mechanisms which require computation of relative depth or motion. V1 and lower areas may therefore play a larger role in color constancy than previously thought.},
  urldate = {2015-07-25},
  booktitle = {Progress in {{Brain Research}}},
  publisher = {{Elsevier}},
  url = {http://www.sciencedirect.com/science/article/pii/S007961230314410X},
  author = {Hurlbert, Anya and Wolf, Kit},
  editor = {Heywood, {and} Blakemore, Milner},
  year = {2004},
  pages = {145-160},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\NIAAGC3X\\Hurlbert and Wolf - 2004 - Color contrast a contributory mechanism to color .pdf;C:\\Users\\cege-user\\Zotero\\storage\\HJAMDFIX\\S007961230314410X.html}
}

