  author = {Tenenbaum, Joshua B. and Griffiths, Thomas L.},
  month = aug,
  year = {2001},
  keywords = {similarity,Bayesian inference,generalization,psychological space,additive clustering,categorization,concept learning,contrast model,features},
  pages = {629-640},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\KIBM2RTP\\Tenenbaum and Griffiths - 2001 - Generalization, similarity, and Bayesian inference.pdf;C:\\Users\\cege-user\\Zotero\\storage\\X7U6I5GG\\595CAA321C9C56270C624057021DE77A.html}
}

