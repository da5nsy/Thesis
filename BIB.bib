@article{lafer-sousa_color_2012,
  title = {Color Tuning in Alert Macaque {{V1}} Assessed with {{fMRI}} and Single-Unit Recording Shows a Bias toward Daylight Colors},
  volume = {29},
  copyright = {\textcopyright{} 2012 Optical Society of America},
  issn = {1520-8532},
  abstract = {Colors defined by the two intermediate directions in color space, ``orange\textendash{}cyan'' and ``lime\textendash{}magenta,'' elicit the same spatiotemporal average response from the two cardinal chromatic channels in the lateral geniculate nucleus (LGN). While we found LGN functional magnetic resonance imaging (fMRI) responses to these pairs of colors were statistically indistinguishable, primary visual cortex (V1) fMRI responses were stronger to orange\textendash{}cyan. Moreover, linear combinations of single-cell responses to cone-isolating stimuli of V1 cone-opponent cells also yielded stronger predicted responses to orange\textendash{}cyan over lime\textendash{}magenta, suggesting these neurons underlie the fMRI result. These observations are consistent with the hypothesis that V1 recombines LGN signals into ``higher-order'' mechanisms tuned to noncardinal color directions. In light of work showing that natural images and daylight samples are biased toward orange\textendash{}cyan, our findings further suggest that V1 is adapted to daylight. V1, especially double-opponent cells, may function to extract spatial information from color boundaries correlated with scene-structure cues, such as shadows lit by ambient blue sky juxtaposed with surfaces reflecting sunshine.},
  language = {EN},
  number = {5},
  journal = {JOSA A},
  doi = {10.1364/JOSAA.29.000657},
  url = {https://www.osapublishing.org/abstract.cfm?uri=josaa-29-5-657},
