@phdthesis{chen_modelling_2006,
  address = {{England}},
  type = {Ph.{{D}}.},
  title = {Modelling of Spectral Reflectance.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {The smoothness of reflectance spectra allows them to be represented by linear models using a small number of parameters. The question of how many parameters are required has been extensively studied by many researchers in the last couple of decades. In this thesis, this question has been thoroughly addressed by applying linear models to different spectral data sets. It was found the performance of a linear model is highly data-set dependent. Different classes of linear models have also been investigated and compared according to the purpose of minimising perceptual differences, and applied to spectral images as a means of spectral compression. A proposed method for efficient representation of spectral images was found to perform better than previous methods. The performance of linear models based on a criterion of an averaged CIELAB {$\Delta$}E*ab of 1 unit has been commonly used for the evaluation of uniform colours. However, a larger tolerance is required for the evaluation of complex images. The performance of linear models of spectral images have been psychophysically studied; the results indicated that the number of basis functions required is also related to image content from which the spectra are derived. Furthermore, a psychophysical study was carried to investigate the effect of spatial structure and complexity changes on the perceptual tolerance of linear-model representations of Mondrian spectral images. It was found the perceptual tolerance do vary with the spatial complexity of the spectral images. Statistical analysis and Fourier-frequency analysis were carried on several sets of natural and man-made reflectance spectra derived from hyperspectral images. It was found that most reflectance spectra are low-pass and are smooth functions of wavelength. A physical model was then proposed and developed using modified Gaussian functions to represent absorption properties of surfaces. Reflectance spectra can be fitted with the physical model with the intention of minimising both spectral and colorimetric reconstruction errors. Though more parameters are required for physical models when compared to conventional linear models, all modelled reflectance spectra are constrained to be in the physical range. A set of artificial spectral reflectance generated from the physical model showed better performance than Munsell spectra in terms of representing natural and man-made reflectance spectra.},
  language = {English},
  urldate = {2019-11-12},
  school = {University of Leeds (United Kingdom)},
  url = {https://search.proquest.com/pqdtglobal/docview/301641928/943C9CCBCCBE4F25PQ/2},
  author = {Chen, Q.},
  year = {2006},
  keywords = {Pure sciences,DXN106588}
}

@phdthesis{li_measurement_1991,
  address = {{United States -- Kansas}},
  type = {Ph.{{D}}.},
  title = {Measurement and Modeling of Vegetative Canopy Spectral Reflectance},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {Remote sensing has become increasingly important for agriculture because it has the potential to provide information about agricultural crops quantitatively, instantaneously, and non-destructively. The measurements and modeling of the canopy reflectance can help gain a better understanding and lead to a better interpretation of remotely sensed data. Studies were conducted on subjects related to canopy reflectance, including leaf orientation, canopy bidirectional reflectance and modeling, and the assessment on the background contribution to the canopy reflectance. Direct measurements of the leaf angle distributions of 14 types of plant canopies were taken using a Spatial Coordinate Apparatus. Four categories of zenith angle distributions were found. These were planophile, plagiophile, erectophile, and uniform. Some canopies were found to have non-uniform leaf azimuth angle distributions and there were differences between the upper and lower parts of the canopies for some species. Field measurements were conducted to examine the reflectance characteristics of selected vegetation canopies as affected by the view and solar angles. Strong relationships were observed between the reflectance factor and the view and solar angles for each canopy although differences were found among canopies. A canopy reflectance model based on radiative transport was examined to assess the effects of the major model input parameters on the canopy reflectance. Comparisons were made between measured and the model generated canopy reflectance. The model indicated that the leaf normal distribution can have large affect on the canopy reflectance in the near-infrared spectral region, as well as the red spectral region when leaf area index is small. Large differences in reflectance were observed by using different leaf scattering functions. The bi-Lambertian leaf scattering model showed the best agreement with the measured soybean canopy reflectance data. The second derivatives of canopy reflectance were examined and used a spectral indices to eliminate soil background contribution to measured reflectance. Two "windows" of second derivatives, \$\textbackslash{}rm \textbackslash{}rho\textbackslash{}sp\{\textbackslash{}prime\textbackslash{}prime\}\textbackslash{}sb\{0.69\textbackslash{}mu m\}\$ and \$\textbackslash{}rm \textbackslash{}rho\textbackslash{}sp\{\textbackslash{}prime\textbackslash{}prime\}\textbackslash{}sb\{0.74\textbackslash{}mu m\}\$, exist which can be used to estimate plant growth parameters such as LAI. The second derivatives showed an advantage over the commonly used near-infrared:red ratio for eliminating the soil background contributions to the composite of soil/canopy spectrum.},
  language = {English},
