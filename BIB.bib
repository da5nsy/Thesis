  month = jul,
  year = {2012},
  keywords = {Building energy performance,Optimization criteria,Visual comfort,Visual performance,Window size},
  pages = {238-245},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\XEMQAGFR\\Ochoa et al. - 2012 - Considerations on design optimization criteria for.pdf;C:\\Users\\cege-user\\Zotero\\storage\\ZPVSFIKK\\S0306261912001419.html}
