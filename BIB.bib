@article{belenky_melanopsin_2003,
  title = {Melanopsin Retinal Ganglion Cells Receive Bipolar and Amacrine Cell Synapses},
  volume = {460},
  copyright = {Copyright \textcopyright{} 2003 Wiley-Liss, Inc.},
  issn = {1096-9861},
  abstract = {Melanopsin is a novel opsin synthesized in a small subset of retinal ganglion cells. Ganglion cells expressing melanopsin are capable of depolarizing in response to light in the absence of rod or cone input and are thus intrinsically light sensitive. Melanopsin ganglion cells convey information regarding general levels of environmental illumination to the suprachiasmatic nucleus, the intergeniculate leaflet, and the pretectum. Typically, retinal ganglion cells communicate information to central visual structures by receiving input from retinal photoreceptors via bipolar and amacrine cells. Because melanopsin ganglion cells do not require synaptic input to generate light-induced signals, these cells need not receive synapses from other neurons in the retina. In this study, we examined the ultrastructure of melanopsin ganglion cells in the mouse retina to determine the type (if any) of synaptic input these cells receive. Melanopsin immunoreaction product was associated primarily with the plasma membrane of (1) perikarya in the ganglion cell layer, (2) dendritic processes in the inner plexiform layer (IPL), and (3) axons in the optic fiber layer. Melanopsin-immunoreactive dendrites in the inner (ON) region of the IPL were postsynaptic to bipolar and amacrine terminals, whereas melanopsin dendrites stratifying in the outer (OFF) region of the IPL received only amacrine terminals. These observations suggested that rod and/or cone signals may be capable of modifying the intrinsic light response in melanopsin-expressing retinal ganglion cells. J. Comp. Neurol. 460:380\textendash{}393, 2003. \textcopyright{} 2003 Wiley-Liss, Inc.},
  language = {en},
  number = {3},
  urldate = {2019-11-24},
  journal = {Journal of Comparative Neurology},
  doi = {10.1002/cne.10652},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cne.10652},
  author = {Belenky, Michael A. and Smeraski, Cynthia A. and Provencio, Ignacio and Sollars, Patricia J. and Pickard, Gary E.},
  year = {2003},
  keywords = {retinohypothalamic tract,circadian rhythm,intergeniculate leaflet,pretectum,suprachiasmatic nucleus},
  pages = {380-393},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\QZV78W47\\Belenky et al. - 2003 - Melanopsin retinal ganglion cells receive bipolar .pdf;C:\\Users\\cege-user\\Zotero\\storage\\G2FGR3YQ\\cne.html}
}

@article{zhang_melanopsin_2012,
  title = {Melanopsin {{Mediates Retrograde Visual Signaling}} in the {{Retina}}},
  volume = {7},
  issn = {1932-6203},
  abstract = {The canonical flow of visual signals proceeds from outer to inner retina (photoreceptors\textrightarrowbipolar cells\textrightarrowganglion cells). However, melanopsin-expressing ganglion cells are photosensitive and functional sustained light signaling to retinal dopaminergic interneurons persists in the absence of rods and cones. Here we show that the sustained-type light response of retinal dopamine neurons requires melanopsin and that the response is mediated by AMPA-type glutamate receptors, defining a retrograde retinal visual signaling pathway that fully reverses the usual flow of light signals in retinal circuits.},
  language = {en},
  number = {8},
  urldate = {2019-11-24},
  journal = {PLOS ONE},
  doi = {10.1371/journal.pone.0042647},
  url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0042647},
  author = {Zhang, Dao-Qi and Belenky, Michael A. and Sollars, Patricia J. and Pickard, Gary E. and McMahon, Douglas G.},
  year = {03-Aug-2012},
  keywords = {Light,Light pulses,Neurons,Photoreceptors,Ganglion cells,Retina,Dopamine,Visual signals},
  pages = {e42647},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\VDK8E9NH\\Zhang et al. - 2012 - Melanopsin Mediates Retrograde Visual Signaling in.pdf;C:\\Users\\cege-user\\Zotero\\storage\\EUPVVAR7\\article.html}
