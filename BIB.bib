  url = {http://www.sciencedirect.com/science/article/pii/S0042698914003034},
  author = {Joyce, Daniel S. and Feigl, Beatrix and Cao, Dingcai and Zele, Andrew J.},
  month = feb,
  year = {2015},
  keywords = {Intrinsically photosensitive retinal ganglion cells (ipRGCs),Melanopsin,Post-illumination pupil response (PIPR),Pupil light reflex,Summation},
  pages = {58-66},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\DFW3W227\\S0042698914003034.html}
}

