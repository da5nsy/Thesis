  doi = {10.1371/journal.pone.0015009},
  url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0015009},
  author = {Semo, Ma'ayan and Gias, Carlos and Ahmado, Ahmad and Sugano, Eriko and Allen, Annette E. and Lawrence, Jean M. and Tomita, Hiroshi and Coffey, Peter J. and Vugler, Anthony A.},
  month = nov,
  year = {2010},
  keywords = {Needs Admin,Mice,Eyes,Mouse models,Pupil,Retina,Animal behavior,Atropine,Axotomy},
  pages = {e15009},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\TPNVPW7J\\Semo et al. - 2010 - Dissecting a Role for Melanopsin in Behavioural Li.pdf;C:\\Users\\cege-user\\Zotero\\storage\\34G4AQRE\\article.html}
