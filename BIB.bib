@article{worthey_opponent-colors_1982,
  title = {Opponent-Colors Approach to Color Rendering},
  volume = {72},
  abstract = {Starting with an opponent-colors formulation of color vision, two parameters, t{\^i} and d{\^i}, may be defined that express an illuminant's ability to realize red\textemdash{}green and blue\textemdash{}yellow contrasts of objects. For instance, calculation of t{\^i} and d{\^i} for daylight shows that on a gray day, color contrasts are actually reduced. By these measures, many common vapor-discharge illuminants systematically distort object colors. Because red-green contrasts contribute to border distinctness, and both types of color contrast contribute to brightness, such systematic distortions probably affect the overall clarity and brightness of what is perceived visually. Experimental data are consistent with this idea.In relation to color-constancy (retinex) experiments, it is approximately true that the visual system discounts the color of an illuminant but not its t{\^i} and d{\^i}.},
