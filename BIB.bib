  author = {Strli{\v c}, Matija and Thickett, David and Taylor, Joel and Cassar, May},
  month = apr,
  year = {2013},
  keywords = {Damage functions,Doseresponse functions,Heritage values,Materials degradation,Preservation modelling,Preventive conservation},
  pages = {80-87},
  file = {C:\\Users\\cege-user\\Zotero\\storage\\ZR4ZD8WN\\Strli et al. - 2013 - Damage functions in heritage science.pdf;C:\\Users\\cege-user\\Zotero\\storage\\2FMWVB3L\\2047058412Y.html}
